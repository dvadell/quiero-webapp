<%- include('includes/header') %>
<body>
  <% results.forEach( result => { %>
    <h1><%= result.title %></h1>
    <p><a href="/q/<%= result.title %>?action=edit">Editar</a></p>
    <p><%= result.description %></p>
    <h3>Pros:</h3>
    <p><%= result.pros %></p>
    <h3>Cons:</h3>
    <p><%= result.cons %></p>
  <% }) %>
</body>
